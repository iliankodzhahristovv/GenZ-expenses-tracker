{
  "name": "nextjs-clean-architecture-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "check": "tsc --noEmit",
    "supabase:start": "npx supabase@latest start",
    "supabase:stop": "npx supabase@latest stop",
    "supabase:status": "npx supabase@latest status",
    "supabase:reset": "npx supabase@latest db reset && pnpm generate:types",
    "supabase:migration:new": "npx supabase@latest migration new",
    "supabase:types": "npx supabase@latest gen types typescript --local > types/database.types.ts",
    "generate:entities": "npx tsx scripts/generate-entity-types.ts",
    "generate:types": "pnpm supabase:types && pnpm generate:entities"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "ai": "^5.0.80",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "inversify": "^7.6.1",
    "lucide-react": "^0.532.0",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.3.0",
    "reflect-metadata": "^0.2.2",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
